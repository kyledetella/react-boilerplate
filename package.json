{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Get up and running with React, quickly",
  "main": "index.js",
  "scripts": {
    "start": "mkdir -p dist/ && serve -p 3045 & watchify -t [ babelify --presets [ react es2015 ] ] src/js/main.js -o dist/bundle.js -v",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Kyle DeTella <kdetella@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "braintree-web": "^2.18.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "immutable": "^3.7.6",
    "mocha": "^2.4.5",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "redux": "^3.3.1",
    "serve": "^1.4.0",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
